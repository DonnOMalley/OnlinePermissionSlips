@model OnlinePermissionSlips.Models.DAL.Student


<div id="studentRow" class="row">
	@using (Html.BeginCollectionItem("Students"))
	{
		<div class="form-group">
			@Html.LabelFor(model => model.FullName, htmlAttributes: new { @class = "col-md-12" })
			<div class="col-md-12">
				@Html.DisplayFor(model => model.FullName, new { htmlAttributes = new { @class = "form-control" } })
			</div>
		</div>
	}
</div>
<hr />

